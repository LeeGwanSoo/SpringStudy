<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

    <mapper namespace="lgs.study.product.mapper.ProductMapper">
        <select id="getProductList" resultType="lgs.study.product.dto.ProductDto">
            SELECT prd_id as prdId
                  ,prd_name as prdName
                  ,price as price
                  ,stock as stock
            FROM product
        </select>
    </mapper>
